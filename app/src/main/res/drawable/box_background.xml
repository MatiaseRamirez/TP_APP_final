<!-- rounded_corners_background.xml -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- Fondo amarillo -->
    <solid android:color="#FFFF00" />
    <!-- Borde rojo -->
    <stroke
        android:width="2dp"
        android:color="#FF0000" />
    <!-- Bordes redondeados -->
    <corners android:radius="20dp" />
</shape>


